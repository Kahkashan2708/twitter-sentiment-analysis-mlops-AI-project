name: twitter-sentiment-analysis

conda_env: conda.yaml

entry_points:
  main:   
    command: >
      python -m src.run_pipeline

  run_pipeline:  
    parameters:
      train_path: {type: str, default: "data/processed/train.csv"}
      test_path: {type: str, default: "data/processed/test.csv"}
      model_out: {type: str, default: "models/model.joblib"}
    command: >
      python -m src.run_pipeline
